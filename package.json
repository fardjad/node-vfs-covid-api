{
  "name": "vfs-covid-api",
  "version": "1.0.0",
  "description": "A RESTful API to get VFS visa application centers' info. For more info, visit https://github.com/fardjad/node-vfs-covid-api",
  "scripts": {
    "pretest": "tsc --noEmit",
    "test": "npm run lab",
    "lab": "lab -S -v -l --transform node_modules/lab-transform-typescript",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc && copyfiles 'src/swagger/templates/**' 'public/**' build/",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run check",
    "postinstall": "npm run compile",
    "start": "node build/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "test:debug": "lab -S -v -l -m 0 --inspect --transform node_modules/lab-transform-typescript"
  },
  "keywords": [
    "covid-19",
    "api",
    "vfs"
  ],
  "author": "Fardjad Davari <public@fardjad.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^20.0.0",
    "@hapi/inert": "^6.0.2",
    "@hapi/vision": "^6.0.1",
    "acorn": "^8.0.1",
    "astring": "^1.4.3",
    "estree-walker": "^2.0.1",
    "got": "^11.8.5",
    "hapi-swagger": "^14.0.0",
    "joi": "^17.2.0",
    "node-worker-threads-pool": "^1.4.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.2",
    "@hapi/lab": "^23.0.0",
    "@types/astring": "^1.3.0",
    "@types/estree": "0.0.45",
    "@types/hapi__hapi": "^19.0.4",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "@types/node": "^12.12.6",
    "copyfiles": "^2.3.0",
    "get-port": "^5.1.1",
    "gts": "^2.0.2",
    "lab-transform-typescript": "^3.0.1",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "~3.9.7"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
