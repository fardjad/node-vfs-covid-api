{
  "name": "vfs-covid-api",
  "version": "1.0.0",
  "description": "Extracts COVID-19 Data from VFS Global website",
  "scripts": {
    "pretest": "tsc --noEmit",
    "test": "run-s lab",
    "lab": "lab --transform node_modules/lab-transform-typescript -v",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "covid-19",
    "api",
    "vfs"
  ],
  "author": "Fardjad Davari <public@fardjad.com>",
  "license": "MIT",
  "dependencies": {
    "acorn": "^7.1.1",
    "astring": "^1.4.3",
    "estree-walker": "^2.0.1",
    "got": "^11.0.2",
    "node-worker-threads-pool": "^1.2.2"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/lab": "^22.0.4",
    "@types/astring": "^1.3.0",
    "@types/estree": "0.0.44",
    "@types/node": "^10.0.3",
    "gts": "^2.0.0",
    "lab-transform-typescript": "^3.0.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~3.8.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
